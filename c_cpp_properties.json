{
    "configurations": [
        {
            "name": "U-Boot",
            "includePath": [
                "${workspaceFolder}/include/*",
                "${workspaceFolder}/arch/sandbox/include/*"
            ],
            "defines" : [
                "__UBOOT__",
                "HOST_ARCH=0x8664"
             ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            },
            "intelliSenseMode": "linux-gcc-x64",
            "forcedInclude": [
                "${workspaceFolder}/include/linux/kconfig.h"
            ]
        }
    ],
    "version": 4
}